<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>e-banque</display-name>
 
 <servlet>
 	<servlet-name>ConnexionServelet</servlet-name>
 	<servlet-class>com.ebanque.servelets.ConnexionServelet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>ConnexionServelet</servlet-name>
 	<url-pattern>/index</url-pattern>
 </servlet-mapping>
 
 <servlet>
 	<servlet-name>AccueilAdminServelt</servlet-name>
 	<servlet-class>com.ebanque.servelets.AccueilAdminServelt</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>AccueilAdminServelt</servlet-name>
 	<url-pattern>/admin/index</url-pattern>
 </servlet-mapping>

 <servlet>
 	<servlet-name>AddAdminServelet</servlet-name>
 	<servlet-class>com.ebanque.servelets.AddAdminServelet</servlet-class>
 </servlet>
 <servlet-mapping>
 	<servlet-name>AddAdminServelet</servlet-name>
 	<url-pattern>/admin/addadmin</url-pattern>
 </servlet-mapping>
 
 
 

</web-app>